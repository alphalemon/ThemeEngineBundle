{
  "name": "alphalemon/theme-engine-bundle",
  "description": "ThemeEngineBundle adds theming feature to any Symfony2 application, adding themes packed as standard Symfony2 Bundles",
  "keywords": ["theme", "themes", "cms"],
  "homepage": "http://alphalemon.com",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "AlphaLemon",
      "email": "info@alphalemon.com"
    }
  ],
  "repositories":[ 
      {
      "phingGit": {
            "type": "package",
            "package": {
                "name": "phing",
                "version": "master",
                "source": {
                "url": "https://github.com/Xosofox/phing.git",
                "type": "git",
                "reference": "dev-master"
                }
            }
        }
    }],
  "require": {
    "symfony/symfony":  ">=2.0.10,<dev-master",
    "propel/propel-bundle": "2.0-dev",
    "alphalemon/al-pagetree-bundle": "dev-master",
    "alphalemon/al-valum-uploader-bundle": "dev-master",
    "propel/propel1": "dev-master",
    "phing": "master"
  },
  "autoload": {
    "psr-0": { "AlphaLemon\\ThemeEngineBundle": "",
               "Themes": "Themes/"
    }
  },
  "target-dir" : "AlphaLemon/ThemeEngineBundle"
}
