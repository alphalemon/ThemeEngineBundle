{
  "name": "alphalemon/theme-engine-bundle",
  "description": "ThemeEngineBundle adds theming feature to any Symfony2 application, adding themes packed as standard Symfony2 Bundles",
  "keywords": ["theme", "themes", "cms"],
  "homepage": "http://alphalemon.com",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "AlphaLemon",
      "email": "info@alphalemon.com"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "symfony/symfony": "2.1.*",
    "doctrine/orm": ">=2.2.3,<2.4-dev",
    "doctrine/doctrine-bundle": "1.0.*",
    "twig/extensions": "1.0.*",
    "symfony/assetic-bundle": "2.1.*",
    "symfony/swiftmailer-bundle": "2.1.*",
    "symfony/monolog-bundle": "2.1.*",
    "sensio/distribution-bundle": "2.1.*",
    "sensio/framework-extra-bundle": "2.1.*",
    "sensio/generator-bundle": "2.1.*",
    "jms/security-extra-bundle": "1.2.*",
    "jms/di-extra-bundle": "1.1.*"
  },
  "require-dev": {
    "mikey179/vfsStream": "dev-master",
    "alphalemon/business-website-theme-bundle": "dev-master"
  },
  "autoload": {
    "psr-0": { "AlphaLemon\\ThemeEngineBundle": ""
    }
  },
  "minimum-stability": "dev",
  "target-dir" : "AlphaLemon/ThemeEngineBundle"
}
